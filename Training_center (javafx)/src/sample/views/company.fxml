<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.CompanyOverviewController">
    <SplitPane dividerPositions="0.3" maxWidth="-Infinity" prefHeight="350.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <AnchorPane minHeight="0.0" minWidth="0.0" prefWidth="100.0">
            <TableView fx:id="companyTableView" AnchorPane.bottomAnchor="-2.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <columns>
                    <TableColumn fx:id="companyColumn" prefWidth="-1.0" text="Company" />
                </columns>
                <columnResizePolicy>
                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                </columnResizePolicy>
            </TableView>
        </AnchorPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" styleClass="background">
            <HBox layoutX="268.0" layoutY="259.0" spacing="10.0" AnchorPane.bottomAnchor="15.0" AnchorPane.rightAnchor="15.0">
                <Button mnemonicParsing="false" onAction="#handleAddCompany" prefHeight="25.0" prefWidth="70.0" text="Add " />
                <Button mnemonicParsing="false" onAction="#handleEditCompany" prefHeight="25.0" prefWidth="70.0" text="Edit" />
                <Button mnemonicParsing="false" onAction="#handleDeleteCompany" prefHeight="25.0" prefWidth="70.0" text="Delete" />
            </HBox>
         <GridPane layoutX="18.0" layoutY="43.0" prefHeight="61.0" prefWidth="386.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="205.0" minWidth="10.0" prefWidth="119.33333333333334" />
             <ColumnConstraints hgrow="SOMETIMES" maxWidth="268.33333333333337" minWidth="10.0" prefWidth="267.33333333333337" />
           </columnConstraints>
           <rowConstraints>
               <RowConstraints maxHeight="37.0" minHeight="10.0" prefHeight="30.333333333333336" vgrow="SOMETIMES" />
             <RowConstraints maxHeight="44.0" minHeight="10.0" prefHeight="30.333333333333336" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <Label text="Account" GridPane.rowIndex="1" />
               <Label fx:id="accountLabel" text="Label" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               <Label text="Name" />
               <Label fx:id="nameLabel" text="Label" GridPane.columnIndex="1" />
            </children>
         </GridPane>
         <Label layoutX="18.0" layoutY="14.0" text="Company's Details">
            <font>
               <Font size="17.0" />
            </font>
         </Label>
         <BarChart fx:id="chart" layoutX="18.0" layoutY="144.0" prefHeight="203.0" prefWidth="358.0">
           <xAxis>
             <CategoryAxis side="BOTTOM" />
           </xAxis>
           <yAxis>
             <NumberAxis prefHeight="163.0" prefWidth="19.0" side="LEFT" />
           </yAxis>
         </BarChart>
         <Button layoutX="18.0" layoutY="112.0" mnemonicParsing="false" onAction="#handleShowEmployees" text="Show Employees" />
        </AnchorPane>
    </SplitPane>
</AnchorPane>
