<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="350.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controllers.LoginController">
    <Button alignment="CENTER" contentDisplay="CENTER" defaultButton="true" layoutX="164.0" layoutY="373.0"
            mnemonicParsing="false" onAction="#handleSignIn" prefHeight="35.0" prefWidth="100.0" text="Sign in"
            AnchorPane.bottomAnchor="25.0" AnchorPane.leftAnchor="150.0" AnchorPane.rightAnchor="150.0"/>
    <GridPane layoutX="60.0" layoutY="113.0" prefHeight="253.0" prefWidth="300.0" AnchorPane.bottomAnchor="100.0"
              AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" AnchorPane.topAnchor="100.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="106.33334096272786" minHeight="10.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="138.66666666666669" minHeight="10.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="151.99998474121094" minHeight="10.0" vgrow="SOMETIMES"/>
            <RowConstraints maxHeight="194.33333333333331" minHeight="10.0" vgrow="SOMETIMES"/>
        </rowConstraints>
        <Label text="Email">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <TextField fx:id="emailField" prefHeight="35.0" prefWidth="215.0" GridPane.rowIndex="1"/>
        <Label text="Password" GridPane.rowIndex="2">
            <font>
                <Font size="15.0"/>
            </font>
        </Label>
        <PasswordField fx:id="passwordField" prefHeight="35.0" prefWidth="300.0" GridPane.rowIndex="3"/>
    </GridPane>
    <Label alignment="CENTER" layoutX="114.0" layoutY="36.0" text="Sign in" AnchorPane.leftAnchor="0.0"
           AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
        <font>
            <Font size="20.0"/>
        </font>
    </Label>
</AnchorPane>
